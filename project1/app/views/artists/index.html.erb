

<h1>Listing artists</h1>

<table>
  <tr>
    <th> Name </th>
    <th> Age </th>
    <th> City </th>
    <th> Genre </th>
  
  </tr>
<% @artists.each do |artist| %>
  <tr>
    <td><%= artist.name %></td>
    <td><%= artist.age %></td>
    <td><%= artist.city %></td>
    <td><%= artist.genre %></td>

    <td><%= link_to 'Show', artist %></td>
    
  </tr>
<% end %>
</table>

  


